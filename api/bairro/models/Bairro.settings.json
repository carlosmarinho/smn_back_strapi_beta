{
  "connection": "default",
  "collectionName": "bairro",
  "info": {
    "name": "bairro",
    "description": ""
  },
  "options": {
    "timestamps": true
  },
  "attributes": {
    "nome": {
      "default": "",
      "type": "string"
    },
    "imagem_destacada": {
      "model": "file",
      "via": "related",
      "plugin": "upload",
      "required": false
    },
    "cidade": {
      "model": "cidade",
      "via": "bairros"
    },
    "descricao": {
      "default": "",
      "type": "text"
    },
    "slug": {
      "default": "",
      "type": "string"
    },
    "wpid": {
      "default": "",
      "type": "integer"
    },
    "menus": {
      "collection": "menu",
      "via": "bairros",
      "dominant": true
    },
    "paginas": {
      "collection": "pagina",
      "via": "bairro"
    },
    "site": {
      "default": "",
      "type": "string"
    },
    "preposicao": {
      "default": "",
      "type": "string"
    },
    "undefined": {
      "collection": "user",
      "via": "bairro",
      "plugin": "users-permissions"
    },
    "guias": {
      "collection": "guia",
      "via": "bairros"
    },
    "eventos": {
      "collection": "evento",
      "via": "bairros"
    },
    "noticia": {
      "collection": "noticia",
      "via": "bairros"
    }
  }
}